datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Blog{
  id Int @id @default(autoincrement())
  author String @default("Phiwa Madinga")
  date DateTime @default(now())
  title String
  description String 
  content String
  slug String
}
